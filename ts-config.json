{
  "compilerOptions": {
    "lib": [
      "es5",
      "es6",
      "es7",
      "es2015",
      "es2016",
      "es2017",
      "es2018",
      "esnext"
    ],
    "target": "es6",
    "module": "commonjs",
    "moduleResolution": "Node",
    "outDir": "./build",
    "esModuleInterop": true,
    "strict": true,
    "declaration": true,
    "declarationMap": true,
    "inlineSourceMap": true,
    "inlineSources": true
  },
  "include": ["./jobs/pruning/*"]
}

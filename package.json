{
  "name": "eh-server-side",
  "version": "1.0.0",
  "description": "Server Side functionality including REST API hosting and module to populate events",
  "main": "index.js",
  "scripts": {
    "start": "ts-node src/api/internal/server.ts",
    "start:client": "ts-node src/api/internal/server.ts",
    "start:updater": "ts-node index.ts",
    "start:storage": "ts-node src/api/internal/storage",
    "test": "jest --coverage --runInBand --forceExit --detectOpenHandles",
    "test:users": "jest users.tests.ts --coverage --runInBand --forceExit --detectOpenHandles --collectCoverageFrom=src/**/users/*.ts",
    "test:events": "jest events.tests.ts --coverage --runInBand --forceExit --detectOpenHandles --collectCoverageFrom=src/**/events/*.ts",
    "test:server": "jest server.tests.ts --runInBand --forceExit --detectOpenHandles",
    "test:watch": "jest --watch --runInBand --detectOpenHandles"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/EventHopper/EHServerSide.git"
  },
  "keywords": [
    "node",
    "express",
    "mongodb",
    "rest"
  ],
  "author": "Ransford Antwi, Kyler Mintah, Batchema Sombie",
  "license": "ISC",
  "bugs": {
    "url": "https://github.com/EventHopper/EHServerSide/issues"
  },
  "homepage": "https://github.com/EventHopper/EHServerSide#readme",
  "dependencies": {
    "@types/dotenv": "^8.2.0",
    "@types/is-url": "^1.2.28",
    "@types/jest": "^26.0.21",
    "@types/mongoose": "^5.10.3",
    "@types/node-geocoder": "^3.24.1",
    "@types/set-interval-async": "^1.0.0",
    "@types/supertest": "^2.0.10",
    "@types/valid-data-url": "^2.0.0",
    "@types/valid-url": "^1.0.3",
    "@types/validator": "^13.1.0",
    "aws-sdk": "^2.736.0",
    "axios": "^0.21.1",
    "body-parser": "^1.19.0",
    "chalk": "^4.1.0",
    "debug": "^4.1.1",
    "dotenv": "^8.2.0",
    "ejs": "^3.1.3",
    "express": "^4.17.1",
    "file-type": "^14.7.1",
    "firebase-admin": "^9.4.1",
    "googleapis": "^68.0.0",
    "got": "^11.5.2",
    "i18n-iso-countries": "^6.0.0",
    "is-url": "^1.2.4",
    "jest-get-type": "^26.3.0",
    "mongoose": "^5.12.1",
    "node-geocoder": "^3.27.0",
    "read-chunk": "^3.2.0",
    "realm": "^10.1.3",
    "set-interval-async": "1.0.33",
    "sinon": "^9.0.3",
    "swagger-ui-express": "^4.1.4",
    "sync-request": "^6.1.0",
    "uuid": "^8.2.0",
    "valid-url": "^1.0.9",
    "validator": "^13.1.1"
  },

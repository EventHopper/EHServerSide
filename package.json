    "@babel/core": "^7.13.10",
    "@babel/preset-env": "^7.11.0",
    "@babel/preset-typescript": "^7.13.0",
    "@types/debug": "^4.1.5",
    "@types/express": "^4.17.7",
    "@types/node": "^14.0.27",
    "@typescript-eslint/eslint-plugin": "^3.8.0",
    "@typescript-eslint/parser": "^3.8.0",
    "babel-jest": "^26.6.3",
    "eslint": "^7.6.0",
    "eslint-config-google": "^0.14.0",
    "jest": "^26.6.3",
    "nyc": "^15.1.0",
    "supertest": "^4.0.2",
    "ts-node": "^9.1.1",
